
pub fn assert(cond: bool, message: string) {
    if cond {}
    else {
        dbg(message)
        panic()
    }
}

pub fn assert(cond: bool) {
    assert(cond, "Assertion failed")
}


pub fn assert_not(cond: bool, message: string) {
    if cond {
        dbg(message)
        panic()
    } else {}
}

pub fn assert_not(cond: bool) {
    assert_not(cond, "Assertion failed")
}

